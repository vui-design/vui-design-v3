{
  "extends": "@vue/tsconfig/tsconfig.web.json",
  "include": ["env.d.ts", "src/**/*", "src/**/*.vue"],
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": ["./src/*"],
      "src/*": ["./src/*"]
    },
    "ignoreDeprecations": "5.0",
    "verbatimModuleSyntax": true,
    "suppressImplicitAnyIndexErrors": true,
    "types": ["node"]
  },
  "references": [
    {
      "path": "./tsconfig.config.json"
    }
  ]
}